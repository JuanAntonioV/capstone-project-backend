{
    "name": "capstone-project-backend",
    "version": "1.0.0",
    "description": "This is a REST API application made with Node.js, Express, and Mysql.",
    "main": "app.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "nodemon ./app.js",
        "make:controller": "node ./scripts/makeController.js",
        "make:model": "npx sequelize-cli model:generate",
        "db:migrate": "npx sequelize-cli db:migrate",
        "db:seed": "npx sequelize-cli db:seed:undo:all && npx sequelize-cli db:seed:all"
    },
    "keywords": [
        "express",
        "sequelize",
        "mysql"
    ],
    "author": "Juan and Team",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "morgan": "^1.10.0",
        "mysql2": "^3.6.5",
        "nodemon": "^3.0.1",
        "sequelize": "^6.35.1"
    },
    "devDependencies": {
        "sequelize-cli": "^6.6.2"
    }
}
